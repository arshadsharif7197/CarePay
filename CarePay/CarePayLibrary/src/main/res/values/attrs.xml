    <declare-styleable name="CustomMenuItem">
        <attr name="imageSource" format="reference" />
        <attr name="labelTextKey" format="string" />
    </declare-styleable>

    <attr name="keepStatusBar" format="boolean"/>
    <attr name="practiceBackgroundColor" format="reference|color" />
    <attr name="newsBackgroundColor" format="reference|color" />
    <attr name="homeButtonSelector" format="reference|color" />
    <attr name="practiceActionBarColor" format="reference|color" />
    <attr name="practiceLogoDrawable" format="reference"/>
