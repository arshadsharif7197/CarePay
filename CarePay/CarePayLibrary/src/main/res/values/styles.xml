        <item name="colorControlNormal">@color/light_gray</item> <!-- color of the underlying bar when no focus -->
        <item name="colorControlActivated">@color/light_gray</item> <!-- hint and underlying bar when focus -->
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>


    <!-- Consent forms -->

    <style name="ConsentActivityTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">21.0sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ConsentActivityDescriptionTextStyle" parent="android:Widget.TextView">
