<resources>

    <!-- Base application theme. -->

    <style name="BreezeButtonStyleDark" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/respons_button_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="BreezeButtonStyleLight" parent="BreezeButtonStyleDark">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:background">@drawable/button_green_border</item>
    </style>

    <!-- Navigation Screen Theme-->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="android:overScrollMode">never</item>
        <item name="android:textViewStyle">@style/DefaultTextViewStyle</item>

        <!-- Hint color and labelMeta color in FALSE state -->
        <item name="android:textColorHint">@color/payne_gray
        </item> <!-- hint color when no focus -->

        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorControlNormal">@color/light_gray
        </item> <!-- color of the underlying bar when no focus -->
        <item name="colorControlActivated">@color/light_gray
        </item> <!-- hint and underlying bar when focus -->

        <!--Action Bar Control-->
        <item name="windowActionModeOverlay">true</item>
        <item name="actionModeStyle">@style/ActionModeStyle</item>
        <item name="actionMenuTextColor">@color/white</item>
        <item name="actionModeCloseDrawable">@drawable/icn_nav_back</item>
    </style>


    <!-- Splash screen theme -->


    <style name="Theme.AppCompat.Translucent" parent="Theme.AppCompat.Light.NoActionBar.FullScreen">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppThemeNoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FF0D91EF</item>
        <item name="colorPrimaryDark">#FF0D91EF</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textViewStyle">@style/DefaultTextViewStyle</item>
    </style>

    <style name="DefaultTextViewStyle" parent="android:Widget.TextView">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cadet_gray</item>
        <item name="android:textSize">@dimen/textview_default_textsize</item>

    </style>

    <style name="actionBarTitle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextLabel" parent="TextAppearance.AppCompat">
        <!-- Hint color and labelMeta color in FALSE state -->
        <item name="android:textColorHint">@color/colorAccent</item>
        <item name="android:textSize">20sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorControlNormal">@color/colorPrimaryDark</item>
        <item name="colorControlActivated">@color/colorPrimaryDark</item>
    </style>

    <style name="Widget.AppCompat.TextView" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/textview_default_textsize</item>
    </style>

    <style name="TextAppearence.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="Theme.AppCompat.Light.NoActionBar.FullScreen" parent="@style/Theme.AppCompat.Light">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimaryDark</item>

    </style>

    <style name="Dialog.FullScreen" parent="Theme.AppCompat.Dialog">
        <item name="android:paddingRight">40dp</item>
        <item name="android:paddingBottom">40dp</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:paddingTop">40dp</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="FloatingLabelStyle">
        <item name="android:textSize">@dimen/hint_floating_size</item>
        <item name="android:textColor">@color/payne_gray</item>
    </style>

    <!-- Custom Keyboard -->

    <style name="keyboard_1_style">
        <!--<item name="android:keyBackground">@drawable/k1_selector</item>-->
        <item name="android:keyTextColor">@android:color/white</item>
        <item name="android:background">#808587</item>
        <item name="android:keyPreviewLayout">@layout/keyboard_preview</item>
    </style>

    <declare-styleable name="SympliTextView">
        <attr name="fontPath" format="string" />

        <!-- Inner shadow settings. -->
        <attr name="innerShadowColor" format="color" />
        <attr name="innerShadowDx" format="dimension" />
        <attr name="innerShadowDy" format="dimension" />
        <attr name="innerShadowRadius" format="dimension" />

        <!-- Outer shadow settings. -->
        <attr name="outerShadowColor" format="color" />
        <attr name="outerShadowDx" format="dimension" />
        <attr name="outerShadowDy" format="dimension" />
        <attr name="outerShadowRadius" format="dimension" />
    </declare-styleable>


    <!-- Common -->

    <style name="TextInputLayoutStyle" parent="TextAppearence.App.TextInputLayout">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="errorTextAppearance">@style/errorTextAppearance</item>
    </style>

    <style name="AlertListTextView" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/respons_detail_label_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">ProximaNova-Regular</item>
    </style>

    <style name="InputLayoutEditTextStyle" parent="Base.Widget.AppCompat.EditText">
        <item name="android:textColor">@color/primary_color_enabled_selector</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/textview_default_textsize</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingBottom">@dimen/dimen_10dp</item>
        <item name="android:paddingTop">@dimen/dimen_10dp</item>
        <item name="android:fontFamily">proximanova_regular</item>
        <item name="android:singleLine">true</item>
        <item name="colorControlNormal">@color/light_gray</item>
        <item name="colorControlActivated">@color/light_gray</item>
        <item name="colorControlHighlight">@color/light_gray</item>
    </style>

    <style name="EditTextStyle" parent="InputLayoutEditTextStyle">
        <item name="android:paddingStart">@dimen/input_text_layout_start_margin</item>
        <item name="android:paddingEnd">@dimen/edit_padding_15</item>
        <item name="android:paddingBottom">@dimen/edit_padding_15</item>
        <item name="android:paddingTop">@dimen/edit_padding_15</item>
    </style>

    <style name="CityEditTextStyle" parent="EditTextStyle">
        <item name="android:digits">abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:maxLength">255</item>
    </style>

    <style name="SigninSignupClickableTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/signup_textview_existing_account_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">@dimen/margintop</item>
    </style>

    <style name="SignupRequireHintStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/signup_optional_label_margin_left_right</item>
        <item name="android:background">@drawable/button_gray_border</item>
        <item name="android:padding">@dimen/dimen_3dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_gravity">end|center_vertical</item>
        <item name="android:textSize">@dimen/signup_optional_label_text_size</item>
    </style>

    <style name="CustomToolbarStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="SearchToolbarStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/colorPrimary</item>
    </style>

    <style name="SearchViewStyleLight" parent="Widget.AppCompat.SearchView">
        <item name="android:editTextColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/white</item>
        <item name="colorControlHighlight">@color/white</item>
    </style>

    <style name="SearchViewStyleDark" parent="Widget.AppCompat.SearchView">
        <item name="android:editTextColor">@color/text_color</item>
        <item name="android:textColorHint">@color/text_color</item>
        <item name="android:textColor">@color/text_color</item>
    </style>

    <style name="SearchViewStyleDark_NoLine" parent="SearchViewStyleDark">
        <item name="colorControlNormal">@color/transparent_apt</item>
        <item name="colorControlActivated">@color/transparent_apt</item>
        <item name="colorControlHighlight">@color/transparent_apt</item>
    </style>

    <!-- Responsibility styles -->

    <style name="ResponsTotalLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/glitter</item>
        <item name="android:textSize">@dimen/respons_total_label_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ResponsTotalStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/respons_total_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ResponsDetailLabelStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/respons_detail_label_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ResponsDetailStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/respons_detail_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ResponsButtonStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginStart">@dimen/respons_button_margin_left_right</item>
        <item name="android:layout_marginEnd">@dimen/respons_button_margin_left_right</item>
        <item name="android:textSize">@dimen/respons_button_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_selector</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ResponsibilityToolbarTextStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/respons_toolbar_title_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:gravity">start</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>


    <!-- Demographics onboarding -->

    <style name="DemographicsDocsButtonStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/demographics_docs_button_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginStart">14dp</item>
        <item name="android:background">@drawable/button_blue_border</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="DemographicsDocsHeaderTitleStyle" parent="android:Widget.TextView">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/demographics_docs_header_title_text_size</item>
    </style>

    <style name="DemographicsDocsHeaderSubtitleStyle" parent="android:Widget.TextView">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cadet_gray</item>
        <item name="android:textSize">@dimen/demographics_docs_button_text_size</item>
    </style>

    <style name="DemographicsDocsHeaderStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginStart">@dimen/demographics_docs_header_left_right_margin
        </item>
        <item name="android:layout_marginEnd">@dimen/demographics_docs_header_left_right_margin
        </item>
        <item name="android:layout_marginTop">@dimen/demographics_docs_header_top</item>
    </style>

    <style name="DemographicsContainerStyle" parent="DemographicsDocsHeaderStyle">
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:layout_marginTop">0dp</item>
    </style>

    <style name="DemographicsDocsDocumentLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/payne_gray</item>
        <item name="android:textSize">@dimen/demographics_docs_number_text_size</item>
        <item name="android:paddingStart">@dimen/input_text_layout_start_margin</item>
        <item name="android:paddingEnd">@dimen/edit_padding_15</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="DemographicsDocsNumberStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/demographics_docs_number_text_size</item>
    </style>

    <style name="DemographicsDocsLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/demographics_docs_number_text_size</item>
        <item name="android:paddingStart">@dimen/signup_optional_label_margin_left_right</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="DemographicsOptionalLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@drawable/button_gray_border</item>
        <item name="android:padding">@dimen/dimen_3dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">@dimen/optionalLabelTextSize</item>
        <item name="android:textColor">@color/optional_gray</item>
    </style>

    <style name="DemographicsRequiredLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_red_border</item>
        <item name="android:padding">@dimen/dimen_3dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">@dimen/text_size_9sp</item>
        <item name="android:textColor">@color/cardinal</item>
    </style>

    <style name="DemographicImageviewStyle">
        <item name="android:radius">3dp</item>
    </style>

    <style name="DemographicsDocsImageButtonContainerStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
