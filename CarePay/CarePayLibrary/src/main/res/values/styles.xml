<resources>

    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="titleTextStyle">@style/actionBarTitle</item>
        <item name="android:overScrollMode">never</item>

    </style>


<!--<splash screen theme></splash>-->

    <style name="Theme.AppCompat.Translucent">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>

    <style name="actionBarTitle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextLabel" parent="TextAppearance.AppCompat">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/colorAccent</item>
        <item name="android:textSize">20sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorControlNormal">@color/colorPrimaryDark</item>
        <item name="colorControlActivated">@color/colorPrimaryDark</item>
    </style>

    <style name="Widget.AppCompat.TextView" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/textview_default_textsize</item>
    </style>

    <style name="TextAppearence.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="Theme.AppCompat.Light.NoActionBar.FullScreen" parent="@style/Theme.AppCompat.Light">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlActivated">@color/colorPrimaryDark</item>

    </style>

    <style name="Dialog.FullScreen" parent="Theme.AppCompat.Dialog">
        <item name="android:paddingRight">40dp</item>
        <item name="android:paddingBottom">40dp</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:paddingTop">40dp</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="FloatingLabelStyle">
        <item name="android:textSize">@dimen/hint_floating_size</item>
        <item name="android:textColor">@color/payne_gray</item>
    </style>

    <!-- Custom Keyboard -->

    <style name="keyboard_1_style">
        <!--<item name="android:keyBackground">@drawable/k1_selector</item>-->
        <item name="android:keyTextColor">@android:color/white</item>
        <item name="android:background">#808587</item>
        <item name="android:keyPreviewLayout">@layout/keyboard_preview</item>
    </style>

    <declare-styleable name="SympliTextView">
        <attr name="fontPath" format="string"/>

        <!-- Inner shadow settings. -->
        <attr name="innerShadowColor" format="color"/>
        <attr name="innerShadowDx" format="dimension"/>
        <attr name="innerShadowDy" format="dimension"/>
        <attr name="innerShadowRadius" format="dimension"/>

        <!-- Outer shadow settings. -->
        <attr name="outerShadowColor" format="color"/>
        <attr name="outerShadowDx" format="dimension"/>
        <attr name="outerShadowDy" format="dimension"/>
        <attr name="outerShadowRadius" format="dimension"/>
    </declare-styleable>


    <!-- Common -->

    <style name="TextInputLayoutStyle" parent="TextAppearence.App.TextInputLayout">
        <item name="android:textColor">@color/bright_cerulean</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="errorTextAppearance">@style/errorTextAppearance</item>
    </style>

    <style name="AlertListTextView" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/respons_detail_label_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">ProximaNova-Regular</item>
    </style>

    <style name="EditTextStyle" parent="android:Widget.EditText">
        <item name="android:textColor">@color/blue_cerulian</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/textview_default_textsize</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">proximanova_regular</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingStart">@dimen/input_text_layout_start_margin</item>
        <item name="android:paddingBottom">@dimen/edit_padding_bottom</item>
        <item name="android:paddingEnd">@dimen/edit_padding_bottom</item>
    </style>

    <style name="CityEditTextStyle" parent="EditTextStyle">
        <item name="android:digits">abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ</item>
        <item name="android:hint">@string/CityEditText</item>
        <item name="android:imeOptions">actionNext</item>
        <item name="android:maxLength">255</item>
    </style>

    <style name="SigninSignupClickableTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/blue_cerulian</item>
        <item name="android:textSize">@dimen/signup_textview_existing_account_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">@dimen/margintop</item>
    </style>

    <style name="SignupRequireHintStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/signup_optional_label_margin_left_right</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:textSize">@dimen/signup_optional_label_text_size</item>
    </style>

    <style name="CustomToolbarStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/blue_cerulian</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- Responsibility styles -->

    <style name="ResponsTotalLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/glitter</item>
        <item name="android:textSize">@dimen/respons_total_label_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ResponsTotalStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/respons_total_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ResponsDetailLabelStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/respons_detail_label_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ResponsDetailStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/respons_detail_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ResponsButtonStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginStart">@dimen/respons_button_margin_left_right</item>
        <item name="android:layout_marginEnd">@dimen/respons_button_margin_left_right</item>
        <item name="android:textSize">@dimen/respons_button_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_selector</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ResponsibilityToolbarTextStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/respons_toolbar_title_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>


    <!-- Demographics onboarding -->

    <style name="DemographicsDocsButtonStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/rich_electric_blue</item>
        <item name="android:textSize">@dimen/demographics_docs_button_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginStart">14dp</item>
        <item name="android:background">@drawable/button_scan_card</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="DemographicsDocsHeaderTitleStyle" parent="android:Widget.TextView">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/demographics_docs_header_title_text_size</item>
    </style>

    <style name="DemographicsDocsHeaderSubtitleStyle" parent="android:Widget.TextView">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/cadet_gray</item>
        <item name="android:textSize">@dimen/demographics_docs_button_text_size</item>
    </style>

    <style name="DemographicsDocsHeaderStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginStart">@dimen/demographics_docs_header_left_right_margin
        </item>
        <item name="android:layout_marginEnd">@dimen/demographics_docs_header_left_right_margin
        </item>
        <item name="android:layout_marginTop">@dimen/demographics_docs_header_top</item>
    </style>

    <style name="DemographicsContainerStyle" parent="DemographicsDocsHeaderStyle">
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:layout_marginTop">0dp</item>
    </style>

    <style name="DemographicsDocsDocumentLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/payne_gray</item>
        <item name="android:textSize">@dimen/demographics_docs_number_text_size</item>
    </style>

    <style name="DemographicsDocsNumberStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/blue_cerulian</item>
        <item name="android:textSize">@dimen/demographics_docs_number_text_size</item>
    </style>

    <style name="DemographicsDocsLabelStyle" parent="android:Widget.TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:layout_marginStart">@dimen/demographics_docs_text_right_margin</item>
        <item name="android:textSize">@dimen/demographics_docs_number_text_size</item>
        <item name="android:paddingStart">@dimen/signup_optional_label_margin_left_right</item>
    </style>

    <style name="DemographicImageviewStyle">
        <item name="android:radius">3dp</item>
    </style>

    <style name="DemographicsDocsImageButtonContainerStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/demographics_docs_image_button_container_height</item>
        <item name="android:layout_marginRight">17dp</item>
        <item name="android:layout_marginLeft">17dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="DemographicsDocsClickableTextStyle" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:textColor">@color/blue_cerulian</item>
        <item name="android:layout_marginRight">14dp</item>
        <item name="android:textSize">@dimen/demographics_docs_clickable_text_size</item>
        <item name="android:layout_marginEnd">@dimen/signup_optional_label_margin_left_right</item>
    </style>

    <style name="DemographicsFragmentsHeadingStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">21.0sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="DemographicsFragmentsSubHeadingStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/cadet_gray</item>
        <item name="android:textSize">13.0sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/review_subtitlemargin</item>
        <item name="android:layout_marginRight">@dimen/review_subtitlemargin</item>
    </style>

    <style name="DemographicsAddRemoveInsuranceStyle" parent="DemographicsDocsButtonStyle">
        <item name="android:layout_marginBottom">@dimen/demographicsAddMedInfoBtnBottomMargin</item>
        <item name="android:layout_marginStart">@dimen/demographics_docs_header_left_right_margin</item>
        <item name="android:background">@drawable/button_scan_card</item>
    </style>

    <style name="DemographicsNextButtonStyle" parent="ResponsButtonStyle">
        <item name="android:layout_marginStart">0dp</item>
        <item name="android:layout_marginEnd">0dp</item>
        <item name="android:fontFamily">GothamRounded-Medium</item>
        <item name="android:textSize">@dimen/demographics_docs_button_text_size</item>
        <item name="android:background">@drawable/agree_button_background</item>
    </style>

    <style name="DemographicsSectionLabelStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/demographics_docs_clickable_text_size</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:layout_marginStart">@dimen/signup_optional_label_margin_left_right</item>
    </style>

    <style name="DemographicsDetailsAddUnlistedStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/demogr_details_container_top</item>
        <item name="android:layout_marginLeft">@dimen/signup_optional_label_margin_left_right</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:textColor">@color/lightSlateGray</item>
        <item name="android:textSize">@dimen/demogr_details_unlisted_text_size</item>
    </style>

    <style name="DemographicsDetailsAllergyStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/demographics_docs_clickable_text_size</item>
        <item name="android:lineSpacingExtra">23dp</item>
        <item name="android:textColor">@color/slateGray</item>
    </style>

    <style name="DemographicsDetailsEditStyle" parent="EditTextStyle">
        <item name="android:paddingStart">@dimen/signup_optional_label_margin_left_right</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:textColor">@color/charcoal</item>
    </style>

    <style name="DemographicsDetailsLabelStyle" parent="DemographicsDocsLabelStyle">
        <item name="android:layout_marginStart">0dp</item>
    </style>

    <!-- Signin Signup -->

    <style name="errorTextAppearance" parent="@android:style/TextAppearance">
        <item name="android:fontFamily">ProximaNova-Semibold</item>
        <item name="android:textColor">@color/harvard_crimson</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="SigninSignupButtonStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/button_selector</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="SigninSignupTextInputStyle" parent="TextInputLayoutStyle"/>

    <style name="SigninSignupEditStyle" parent="EditTextStyle">
        <item name="android:imeOptions">actionNext</item>
        <item name="android:textColorHint">@color/lightSlateGray</item>
    </style>

    <style name="SigninNewAccountButtonStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@drawable/button_default_background</item>
        <item name="android:textColor">@color/blue_cerulian</item>
    </style>

    <style name="SignupInstructionsStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/signup_button_textsize</item>
        <item name="android:textColor">@color/cadet_gray</item>
        <item name="android:layout_marginTop">@dimen/sign_up_margin_top</item>
        <item name="android:padding">0dp</item>
        <item name="android:layout_margin">0dp</item>
    </style>

    <!-- Demographics Review Screens Style>-->

    <style name="reviewDemographicBlue." parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/review_section_header_textsize</item>
        <item name="android:layout_marginTop">@dimen/review_section_header</item>
        <item name="android:layout_marginLeft">@dimen/review_textview_leftmargin</item>
    </style>
    <style name="reviewDemographiclabel." parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/review_textlable_textsize</item>
        <item name="android:layout_marginTop">@dimen/review_lablemargintop</item>
        <item name="android:layout_marginLeft">@dimen/review_textview_leftmargin</item>
    </style>

    <style name="reviewDemographictext." parent="android:Widget.TextView">
        <item name="android:textColor">@color/lightSlateGray</item>
        <item name="android:textSize">@dimen/textview_default_textsize</item>
        <item name="android:layout_marginTop">@dimen/review_texviewmargin_top</item>
        <item name="android:layout_marginLeft">@dimen/review_textview_leftmargin</item>
    </style>
    <style name="Review_Screen_View_Style">
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/light_gray</item>
        <item name="android:layout_marginTop">@dimen/review_viewmargin_top</item>
    </style>
    <style name="reviewDemographicrelativelayout" parent="android:Widget.TextView">
        <item name="android:textColor">@color/charcoal</item>
        <item name="android:textSize">@dimen/textview_default_textsize</item>
        <item name="android:layout_marginTop">@dimen/review_center_margin_top_bottom</item>
        <item name="android:layout_marginBottom">@dimen/review_center_margin_top_bottom</item>
        <item name="android:layout_marginLeft">@dimen/review_textview_leftmargin</item>
    </style>

    <!-- Navigation Screen Theme-->
    <style name="NavigationTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="android:overScrollMode">never</item>

        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/payne_gray</item> <!-- hint color when no focus -->

