    <style name="ConsentForm_GenderStyle" parent="android:Widget.TextView.SpinnerItem">
        <item name="android:text">@string/choose_gender</item>
        <item name="android:layout_marginTop">@dimen/consent_view_margin_top_bottom</item>
        <item name="android:layout_marginLeft">@dimen/consent_spinner_left_margin</item>
        <item name="android:layout_alignBaseline">@id/minor_gender</item>
