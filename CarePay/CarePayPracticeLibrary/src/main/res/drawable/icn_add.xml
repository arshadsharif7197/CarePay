<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:topRightRadius="@dimen/dimen_100dp"
                     android:bottomRightRadius="@dimen/dimen_100dp"/>
            <size android:height="@dimen/dimen_36dp"
                  android:width="@dimen/dimen_50dp"/>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="@dimen/dimen_2dp"
                    android:color="@color/colorPrimary"/>
        </shape>
    </item>
    <item android:top="@dimen/dimen_10dp"
          android:bottom="@dimen/dimen_10dp"
          android:left="@dimen/dimen_15dp"
          android:right="@dimen/dimen_15dp">
        <rotate android:fromDegrees="90">
            <shape android:shape="line" android:thickness="@dimen/dimen_2dp">
                <stroke android:width="@dimen/dimen_2dp" android:color="@color/colorPrimary"/>
            </shape>
        </rotate>
    </item>
    <item android:top="@dimen/dimen_10dp"
          android:bottom="@dimen/dimen_10dp"
          android:left="@dimen/dimen_15dp"
          android:right="@dimen/dimen_15dp">
        <shape android:shape="line" android:thickness="@dimen/dimen_2dp">
            <stroke android:width="@dimen/dimen_2dp" android:color="@color/colorPrimary"/>
        </shape>
    </item>

</layer-list>